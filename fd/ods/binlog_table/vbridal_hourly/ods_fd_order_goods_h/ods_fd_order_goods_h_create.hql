  `status_id` string COMMENT '商品新旧状态',
  `added_fee` decimal(13,4) COMMENT '税率',
  `custom_fee` decimal(13,4) COMMENT '自定义尺寸的费用',
  `custom_fee_exchange` decimal(13,4) COMMENT '',
  `plussize_fee` decimal(13,4) COMMENT '大尺码加钱',
  `plussize_fee_exchange` decimal(13,4) COMMENT '',
  `rush_order_fee` decimal(13,4) COMMENT 'rush order fee',
