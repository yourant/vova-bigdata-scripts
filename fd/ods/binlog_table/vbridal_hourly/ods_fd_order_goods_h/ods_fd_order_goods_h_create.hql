  `display_plussize_fee_exchange` decimal(13,4) COMMENT '显示用加大码费',
  `display_rush_order_fee_exchange` decimal(13,4) COMMENT '显示用加急费',
  `display_wrap_price_exchange` decimal(13,4) COMMENT '显示用披肩费',
