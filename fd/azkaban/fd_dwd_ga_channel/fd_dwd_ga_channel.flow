---
config:
  failure.emails: jhlu@i9i8.com
  retries: 10
  retry.backoff: 300000

nodes:
  - name: fd_daily_job_start
    type: noop

  - name: fd_dwd_daily_flow
    type: flow
    config:
      prop: value
    nodes:
      - name: fd_dwd_daily_flow_start
        type: noop

      - name: judge_ods_fd_snowplow_view_event
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh ods_fd_snowplow_view_event_hour
        dependsOn:
          - fd_dwd_daily_flow_start

      - name: dwd_fd_ga_channel
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/fd/dwd/dwd_fd_ga_channel/run.sh "${pt}"
        dependsOn:
          - judge_ods_fd_snowplow_view_event

      - name: fd_dwd_daily_flow_end
        type: noop
        dependsOn:
          - dwd_fd_ga_channel

    dependsOn:
      - fd_daily_job_start

  - name: fd_hour_job_end
    type: noop
    dependsOn:
      - fd_dwd_daily_flow

