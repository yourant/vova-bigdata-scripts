---
config:
  failure.emails: jhlu@i9i8.com
  retries: 10
  retry.backoff: 300000

nodes:
  - name: fd_job_start
    type: noop

  - name: ads_fd_goods_inspired
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/fd/ads/ads_fd_goods_inspired/run.sh "${pt}"
    dependsOn:
      - fd_job_start

  - name: sync_ads_fd_goods_inspired
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/fd/ads/ads_fd_goods_inspired/sqoop.sh "${pt}"
    dependsOn:
      - ads_fd_goods_inspired

  - name: fd_job_end
    type: noop
    dependsOn:
      - sync_ads_fd_goods_inspired
