---
config:
  failure.emails: yunling@i9i8.com
  retries: 5
  retry.backoff: 300000

nodes:
  - name: fd_erp_rpt_start
    type: noop

  - name: erp_unsale_rpt_flow
    type: flow
    config:
      prop: value
    nodes:
      - name: erp_unsale_rpt_flow_start
        type: noop
      
      - name: judge_fd_dw_erp_ods_inc_end
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh fd_dw_erp_ods_inc_end
        dependsOn:
          - erp_unsale_rpt_flow_start

      - name: dwd_fd_erp_unsale_goods_info
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/fd/dwb/dwb_fd_erp_14d_avg_sale/run.sh dwd_fd_erp_unsale_goods_info
        dependsOn:
          - judge_fd_dw_erp_ods_inc_end

      - name: dwd_fd_spring_festival_stock_up_info
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/fd/dwb/dwb_fd_erp_14d_avg_sale/run.sh dwd_fd_spring_festival_stock_up_info
        dependsOn:
          - judge_fd_dw_erp_ods_inc_end

      - name: dwb_fd_erp_unsale_detail
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/fd/dwb/dwb_fd_erp_14d_avg_sale/run.sh dwb_fd_erp_unsale_detail
        dependsOn:
          - dwd_fd_erp_unsale_goods_info
          - dwd_fd_spring_festival_stock_up_info

      - name: dwb_fd_erp_unsale_rpt
        type: command
        config:
          command: sh /mnt/vova-bigdata-scripts/fd/dwb/dwb_fd_erp_unsale_rpt/run.sh dwb_fd_erp_unsale_rpt
        dependsOn:
          - dwb_fd_erp_unsale_detail

      - name: erp_unsale_rpt_flow_end
        type: noop
        dependsOn:
          - dwb_fd_erp_unsale_rpt

    dependsOn:
      - fd_erp_rpt_start

  - name: fd_erp_rpt_end
    type: noop
    dependsOn:
      - erp_unsale_rpt_flow
