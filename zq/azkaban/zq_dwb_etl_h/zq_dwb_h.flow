---
config:
  retries: 10
  retry.backoff: 600000

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - dwb_zq_main_process

  - name: start_flag
    type: command
    config:
      command: echo "start job"

  - name: dwb_zq_main_process
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/zq/dwb/main_process/update.sh
      retries: 20
      retry.backoff: 120000
    dependsOn:
      - judge_ods_zq_order_info_h
      - judge_dim_zq_domain_userid
      - judge_dim_zq_site

  - name: judge_dim_zq_domain_userid
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh dim_zq_domain_userid
      retries: 20
      retry.backoff: 120000
    dependsOn:
      - start_flag

  - name: judge_dim_zq_site
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh dim_zq_site
      retries: 20
      retry.backoff: 120000
    dependsOn:
      - start_flag

  - name: judge_ods_zq_order_info_h
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh fn_order_info
      retries: 20
      retry.backoff: 120000
    dependsOn:
      - start_flag
