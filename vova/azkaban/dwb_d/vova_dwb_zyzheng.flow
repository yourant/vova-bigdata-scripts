    dependsOn:
      - start_flag

  - name: judge_dwd_vova_web_fact_order_cause
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh dwd_vova_web_fact_order_cause
      retries: 10
      retry.backoff: 300000
    dependsOn:
