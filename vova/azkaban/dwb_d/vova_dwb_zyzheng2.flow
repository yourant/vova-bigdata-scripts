      retries: 10
      retry.backoff: 300000
    dependsOn:
      - start_flag

  - name: judge_ods_vova_activity_coupon
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/common/judge_check.sh vova_activity_coupon
      retries: 10
      retry.backoff: 300000
    dependsOn:
      - start_flag

  - name: judge_ods_vova_ads_lower_price_goods_red_packet
