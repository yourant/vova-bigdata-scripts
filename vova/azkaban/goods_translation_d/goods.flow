---
config:
  retries: 4
  retry.backoff: 600000

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - buyer_push_portrait_export

  - name: start_flag
    type: command
    config:
      command: echo "start job"

