---
config:
  failure.emails: yzhang13@vova.com.hk
  retries: 5
  retry.backoff: 600000

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - goods_knock_d

  - name: start_flag
    type: command
    config:
      command: echo "start job"

  - name: judge_dim_goods
    type: command
    config:
      command: sh /mnt/vova-bd-scripts/ods/judge_check.sh dim_goods
      retries: 100
      retry.backoff: 120000
    dependsOn:
      - start_flag

  - name: judge_fact_pay
    type: command
    config:
      command: sh /mnt/vova-bd-scripts/ods/judge_check.sh fact_pay
      retries: 100
      retry.backoff: 120000
    dependsOn:
      - start_flag

  - name: judge_fact_log_goods_impression
    type: command
    config:
      command: sh /mnt/vova-bd-scripts/ods/judge_check.sh fact_log_goods_impression
      retries: 100
      retry.backoff: 120000
    dependsOn:
      - start_flag

  - name: judge_vova_rec_gid_pic_similar
    type: command
    config:
      command: sh /mnt/vova-bd-scripts/ods/judge_check.sh vova_rec_gid_pic_similar
      retries: 100
      retry.backoff: 120000
    dependsOn:
      - start_flag

  - name: goods_knock_d
    type: command
    config:
      command:  sh /mnt/vova-bd-scripts/rpt/goods_knock/update.sh
      retries: 5
    dependsOn:
      - judge_vova_rec_gid_pic_similar
      - judge_fact_log_goods_impression
      - judge_fact_pay
      - judge_dim_goods
