---
config:
  failure.emails: kaicheng@vova.com.hk
  retries: 5
  retry.backoff: 600000

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - mlb_vova_rec_u2i

  - name: start_flag
    type: command
    config:
      command: echo "start job"

  - name: mlb_vova_rec_u2i
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/vova/mlb/sqoop_export_common/mlb_vova_rec_u2i.sh '${freedoms}'
      retries: 45
      retry.backoff: 1200000
    dependsOn:
      - start_flag