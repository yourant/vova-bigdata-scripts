---
config:
  retries: 5
  retry.backoff: 600000
  failure.emails: ted.wan@vova.com.hk,yzhang13@vova.com.hk,kaicheng@vova.com.hk

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - sqoop_ads_vova_goods_white_bg_img_res_b


  - name: start_flag
    type: command
    config:
      command: echo "start job"

## judge ############################################

# 获取 b 组 算法消息
  - name: judge_message_get_ads_vova_goods_white_bg_img_res_b_cv
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/common/job_message_get.sh --jname=ads_vova_goods_white_bg_img_res_b_cv --from=cv --to=data --valid_hour=3
      retries: 30
      retry.backoff: 1200000
    dependsOn:
      - start_flag

## sqoop ############################################

  - name: sqoop_ads_vova_goods_white_bg_img_res_b
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/vova/ads/goods_white_bg_img/sqoop_export_b.sh
      retries: 5
      retry.backoff: 600000
    dependsOn:
      - judge_message_get_ads_vova_goods_white_bg_img_res_b_cv



