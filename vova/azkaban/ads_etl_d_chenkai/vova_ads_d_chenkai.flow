---
config:
  retries: 5
  retry.backoff: 600000
  failure.emails: ted.wan@vova.com.hk,yzhang13@vova.com.hk,kaicheng@vova.com.hk

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - ads_vova_mct_manage_module


  - name: start_flag
    type: command
    config:
      command: echo "start job"

## judge ############################################
