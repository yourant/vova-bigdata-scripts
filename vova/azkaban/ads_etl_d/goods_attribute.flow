      retry.backoff: 300000
    dependsOn:
      - judge_goods_attribute_watch
      - judge_goods_attribute_household
      - judge_goods_attribute_underwear
      - judge_goods_attribute_mobile
      - judge_goods_attribute_shoe
      - judge_goods_attribute_bag
      - judge_goods_attribute_electronics
