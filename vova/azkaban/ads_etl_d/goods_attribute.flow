---
config:
  retries: 5
  retry.backoff: 600000
  failure.emails: ted.wan@vova.com.hk,yzhang13@vova.com.hk,kaicheng@vova.com.hk

nodes:
  - name: end_flag
    type: command
    config:
      command: echo "stop job"
    dependsOn:
      - ads_vova_goods_pre_attribute_data


  - name: start_flag
    type: command
    config:
      command: echo "start job"


  - name: judge_goods_attribute_watch
    type: command
    config:
      command: sh /mnt/vova-bigdata-scripts/vova/ads/goods_attribute/update.sh s3://vova-chatbot/kg/all_data/watches dwd.dwd_vova_goods_attribute_watch 2
      retries: 5
      retry.backoff: 6000
    dependsOn:
      - start_flag

  - name: judge_goods_attribute_household
